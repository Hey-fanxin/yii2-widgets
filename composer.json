{
    "name": "limefamily/yii2-widgets",
    "description": "An asset bundle for bootstrap3-widgets for Yii 2.0 framework.",
    "keywords": [
        "widgets", "yii2-widget", "components"
    ],
    "homepage": "https://github.com/namebjp/yii2-widget",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "bianjunping",
            "email": "bianjunping@limefamily.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "limefamily/static-theme": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "limefamily\\widgets\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
